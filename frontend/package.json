{
  "name": "frontend",
  "version": "0.0.0",
  "engines": {
    "node": "18.14.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "gh-pages": "vite build --outDir ../docs",
    "gh-pages:clean": "cd ../docs; ls . | grep -v -E 'README.md|.nojekyll' | xargs rm -rf",
    "test": "jest",
    "eslint": "eslint --ext \".js,.ts,.vue\" .",
    "eslint:fix": "eslint --ext \".js,.ts,.vue\" .",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "dependencies": {
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/test-utils": "^2.0.0-rc.10",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-vue": "^9.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "sass": "^1.32.7",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "vue-jest": "^5.0.0-alpha.10"
  }
}
